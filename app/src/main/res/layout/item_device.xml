        stv:sShapeType="oval"
        stv:sSolidColor="@color/colorWhite"
        stv:sStrokeColor="@color/colorPrimary"
        stv:sStrokeWidth="0.5dp"
        tools:ignore="SmallSp" />
</RelativeLayout>